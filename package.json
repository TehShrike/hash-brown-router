  },
  "homepage": "https://github.com/TehShrike/hash-brown-router",
  "devDependencies": {
    "browser-run": "3.5.0",
    "browserify": "~11.0.0",
    "tap-browser-color": "^0.1.2",
    "tape": "~4.1.0",
    "tape-catch": "1.0.4",
    "tape-run": "2.1.4"
  },
  "dependencies": {
    "eventemitter3": "2.0.3",
